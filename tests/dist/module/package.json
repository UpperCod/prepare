{
    "name": "demo",
    "dependencies": {
        "atomico": "latest"
    },
    "peerDependencies": {
        "@atomico/react": "*",
        "@atomico/vue": "*"
    },
    "peerDependenciesMeta": {
        "@atomico/react": {
            "optional": true
        },
        "@atomico/vue": {
            "optional": true
        }
    },
    "main": "./tests/module/atomico.js",
    "module": "./tests/module/atomico.js",
    "types": "./tests/module/atomico.d.ts",
    "exports": {
        "./atomico": {
            "default": "./tests/module/atomico.js",
            "types": "./tests/module/atomico.d.ts"
        },
        "./components/a": {
            "default": "./tests/module/components/a/a.js"
        },
        "./components/c": {
            "default": "./tests/module/components/c/c.js",
            "types": "./tests/module/components/c/c.d.ts"
        },
        "./components/b": {
            "default": "./tests/module/components/b/b.js"
        },
        "./react": {
            "default": "./tests/dist/module/react.js",
            "types": "./tests/dist/module/react.d.ts"
        },
        "./preact": {
            "default": "./tests/dist/module/preact.js",
            "types": "./tests/dist/module/preact.d.ts"
        },
        "./vue": {
            "default": "./tests/dist/module/vue.js",
            "types": "./tests/dist/module/vue.d.ts"
        },
        ".": {
            "default": "./tests/module/atomico.js",
            "types": "./tests/module/atomico.d.ts"
        },
        "./package.json": {
            "default": "./tests/module/package.json"
        }
    },
    "typesVersions": {
        "*": {
            "atomico": [
                "./tests/module/atomico.d.ts"
            ],
            "components/c": [
                "./tests/module/components/c/c.d.ts"
            ],
            "react": [
                "./tests/dist/module/react.d.ts"
            ],
            "preact": [
                "./tests/dist/module/preact.d.ts"
            ],
            "vue": [
                "./tests/dist/module/vue.d.ts"
            ]
        }
    }
}
