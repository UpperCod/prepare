{
    "name": "@atomico/exports",
    "type": "module",
    "version": "0.35.0",
    "description": "It allows to generate an export to quickly publish components created with Atomico in NPM",
    "main": "./src/cli.js",
    "module": "./src/cli.js",
    "types": "./types/cli.d.ts",
    "workspaces": [
        "tests/*"
    ],
    "exports": {
        ".": "./src/cli.js"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "types": "tsc",
        "test": "ava",
        "prepare": "npm run test && npm run types"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/atomicojs/exports.git"
    },
    "author": "UpperCod",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/atomicojs/exports/issues"
    },
    "homepage": "https://github.com/atomicojs/exports#readme",
    "dependencies": {
        "@atomico/pipeline": "latest",
        "@types/node": "^14.14.41",
        "@uppercod/esbuild-meta-url": "latest",
        "@uppercod/indentation": "latest",
        "acorn": "^8.3.0",
        "acorn-walk": "^8.1.0",
        "atomico": "latest",
        "cac": "^6.7.2",
        "esbuild": "latest",
        "fast-glob": "latest",
        "postcss": "latest",
        "postcss-import": "latest",
        "postcss-load-config": "latest"
    },
    "devDependencies": {
        "@atomico/react": "latest",
        "@uppercod/hash": "^1.1.1",
        "ava": "^3.10.1",
        "builtin-modules": "^3.1.0",
        "normalize.css": "^8.0.1",
        "postcss": "^8.3.5",
        "postcss-import": "^14.0.2",
        "postcss-load-config": "^3.1.0",
        "preact": "^10.8.2",
        "react": "^18.2.0",
        "rollup": "^2.21.0",
        "typescript": "^4.2.4",
        "ustyler": "^1.0.1"
    },
    "peerDependencies": {
        "@atomico/pipeline": "latest",
        "@atomico/react": "latest",
        "atomico": "latest",
        "esbuild": "latest",
        "postcss": "latest",
        "postcss-import": "latest",
        "postcss-load-config": "latest"
    },
    "peerDependenciesMeta": {
        "@atomico/react": {
            "optional": true
        }
    },
    "ava": {
        "files": [
            "tests/**/*.test.js"
        ]
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": false,
        "endOfLine": "auto"
    },
    "bin": {
        "exports": "./src/cli.js"
    },
    "postcss": {
        "plugins": {
            "postcss-import": {}
        }
    }
}
