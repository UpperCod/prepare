{
    "name": "@atomico/exports",
    "type": "module",
    "version": "0.29.0",
    "description": "It allows to generate an export to quickly publish components created with Atomico in NPM",
    "main": "./src/cli.js",
    "module": "./src/cli.js",
    "types": "./cli.d.ts",
    "exports": {
        ".": "./src/cli.js"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "types": "tsc",
        "test": "ava",
        "prepare": "npm run test && npm run types"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/UpperCod/pkg_name.git"
    },
    "author": "UpperCod",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/UpperCod/pkg_name/issues"
    },
    "homepage": "https://github.com/UpperCod/pkg_name#readme",
    "dependencies": {
        "@atomico/pipeline": "^0.0.1",
        "@types/node": "^14.14.41",
        "@uppercod/esbuild-meta-url": "latest",
        "@uppercod/indentation": "latest",
        "acorn": "^8.3.0",
        "acorn-walk": "^8.1.0",
        "atomico": "latest",
        "cac": "^6.7.2",
        "csso": "^4.2.0",
        "esbuild": "^0.14.38",
        "fast-glob": "^3.2.5",
        "postcss": "^8.3.5",
        "postcss-import": "^14.0.2",
        "postcss-load-config": "^3.1.0"
    },
    "devDependencies": {
        "@atomico/react": "latest",
        "@uppercod/hash": "^1.1.1",
        "ava": "^3.10.1",
        "builtin-modules": "^3.1.0",
        "csso": "^4.2.0",
        "normalize.css": "^8.0.1",
        "postcss": "^8.3.5",
        "postcss-import": "^14.0.2",
        "postcss-load-config": "^3.1.0",
        "rollup": "^2.21.0",
        "typescript": "^4.2.4"
    },
    "peerDependencies": {
        "csso": "^4.2.0",
        "esbuild": "^0.14.38",
        "@atomico/react": "latest",
        "postcss": "^8.3.5",
        "postcss-import": "^14.0.2",
        "postcss-load-config": "^3.1.0"
    },
    "peerDependenciesMeta": {
        "@atomico/react": {
            "optional": true
        }
    },
    "ava": {
        "files": [
            "tests/**/*.test.js"
        ]
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": false,
        "endOfLine": "auto"
    },
    "bin": {
        "exports": "./src/cli.js"
    },
    "postcss": {
        "plugins": {
            "postcss-import": {}
        }
    }
}
